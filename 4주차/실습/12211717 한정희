//퀴즈 4
#include <iostream>
#include <iomanip>

using namespace std;

int main() {
	int i, num1, num2, num3{ 0 }, counter{ 0 };

	cin >> num1 >> num2;

	if (num1 > num2) {
		num3 = num1;
		num1 = num2;
		num2 = num3;
	}

	for (i = 1; i <= 100; i++) {
		if (i % num1 == 0) {
			cout << setw(5) << i;
			counter += 1;

			if (counter % 5 == 0)
				cout << "\n";
		}

		if (i % num1 == i % num2) {
			continue;
		}
		else if (i % num2 == 0) {
			cout << setw(5) << i;
			counter += 1;

			if (counter % 5 == 0)
				cout << "\n";
		}


	}

	cout << "\ntotal : " << counter;

	return 0;
}

